
part {
	name: "background";
	type: RECT;
	mouse_events: 0;

	description {
		state: "default" 0.0;
		color: 0 0 0 255;	/* fallback color */
	}
}
 
part { 
	name: "background_image";
	type: GRADIENT;
	mouse_events: 0;

	description {
		state: "default" 0.0;
		gradient.spectrum: "radial_gradient";
		gradient.type: "radial";
		fill.origin.relative: 0.5 0.5;
	}
}

part { 
	name: "over";
	type: RECT;
	mouse_events: 0;

	description {
		state: "default" 0.0;
		color: 7 7 7 255;
	}
}

