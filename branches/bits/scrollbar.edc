group
{
	name: "/ewl/vscrollbar";
	min: 17 36;
	max: 17 99999;
	parts
	{
		part
		{
			name: "scrollbar_base";
			type: RECT;
			mouse_events: 0;
			description
			{
				state: "default" 0.0;
				rel1
				{
                                        relative: 0.0 0.0;
                                        offset: 0 0;
                                }
                                rel2
                                {
                                        relative: 1.0 1.0;
                                        offset: -1 -1;
