				fixed: 1 1;
				min: 13 17;
				max: 13 99999;
				rel1
				{
					to: "scrollbar_base";
				}
				rel2
				{
					to: "scrollbar_base";
				}
				image
				{
					normal: "sf_barv1.png";
					border: 6 6 6 6;
				}
				fill
				{
					smooth: 0;
				}
			}
			description
			{
				state: "clicked" 0.0;
				inherit: "default" 0.0;
				color: 215 215 215 255;
			}
		}
		part
		{
			name: "thumb";
			description
			{
				state: "default" 0.0;
				min: 7 11;
				max: 7 11;
				fixed: 1 1;
				rel1
				{
					relative: 0.5 0.5;
					offset: 0 0;
				}
				rel2
				{
					relative: 0.5 0.5;
					offset: 0 0;
				}
				image
				{
					normal: "sf_barv_thumb.png";
					border: 2 2 2 2;
				}
			}
			description
			{
				state: "clicked" 0.0;
				inherit: "default" 0.0;
				color: 215 215 215 255;
			}	
		}
	}
	programs
	{
		program
		{
			name: "clicked";
			signal: "mouse,down,*";
			source: "EWL";
			action: STATE_SET "clicked" 0.0;
			target: "scrollbar";
			target: "thumb";
		}
                program
                {
                        name: "default";
			signal: "mouse,up,*";
                        source: "EWL";
                        action: STATE_SET "default" 0.0;
                        target: "scrollbar";
			target: "thumb";
                }
        }
}

