
part {
	name: "item_clip";
	type: RECT;

	description {
		state: "default" 0.0;

	r1 { to: "e.swallow.content"; offset: 1 0; }
	r2 { to: "e.swallow.content"; offset: -2 -2; }
	}
}
  
part {
	name: "e.swallow.content";
	type: SWALLOW;
	clip_to: "item_clip";
	mouse_events: 0;

	description {
		state: "default" 0.0;

	r1 { to: "border"; offset: 0 1; }
	r2 { to: "border"; offset: -1 -1; }

	color: 0 0 0 0;
	}
}
