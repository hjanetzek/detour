group {
	name: "/ewl/border";
	data {
		item: "inset/top" "8";
		item: "inset/bottom" "8";
		item: "inset/left" "8";
		item: "inset/right" "8";
	}
	parts {
		part {
			name: "frame";
			mouse_events: 0;
			description {
				state: "default" 0.0;
				rel1 {
					relative: 0 0;
					offset: 0 0;
				}
				rel2 {
					relative: 1 1;
					offset: -1 -1;
				}
				image {
					normal: "frame_outline.png";
					border: 9 9 9 9;
				}
				fill {
					smooth: 0;
				}
			}
		}	
	}
}
