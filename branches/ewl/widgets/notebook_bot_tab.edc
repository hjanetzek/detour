			relative: 0.0 0.0;
			offset: -2 -5;
		}

		rel2 {
			relative: 1.0 1.0;
			offset: 2 -1;
		}

		image {
			normal: "e17_button_sm_disabled.png";
			border: 8 8 8 8;
		}
	}

	description {
		state: "selected" 0.0;
		inherit: "default" 0.0;
		visible: 1;

		image {
			normal: "ewl_notebook_tab_bottom_selected.png";
			border: 8 8 8 8;
		}
	}
}

