
group
{
	name: "ewl/statusbar";
	min: 0 17;
	max: 99999 17;

	data
	{
		item: "inset" 3;
	}

	parts {

		part
		{
			name: "background";
			mouse_events: 0;

			description
			{
				state: "default" 0.0;

				rel1
				{
					relative: 0.0 0.0;
					offset: 0 0;
				}

				rel2
				{
					relative: 1.0 1.0;
					offset: -1 -1;
				}

				image
				{
					normal: "status_bar.png";
					border: 0 0 3 3;
				}
				
				fill
				{
					smooth: 0;
				}
			}
		}
	}
}
