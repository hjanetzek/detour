#define ICON(GROUP_NAME,IMAGE) \
group { \
	name: GROUP_NAME; \
	max: 64 64; \
	parts { \
		part { \
			name: "icon"; \
			mouse_events: 0; \
			description { \
				state: "default" 0.0; \
				aspect: 1.0 1.0; \
				aspect_preference: BOTH; \
				max: 64 64; \
				image { \
					normal: IMAGE; \
				} \
	    		} \
   		} \
    	} \
}

