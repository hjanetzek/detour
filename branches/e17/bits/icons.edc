group {
	name: "e/icons/enlightenment/gradient_h";
	max: 24 24;

	parts {
		part {
			name: "bg";
			mouse_events: 0;
			type: IMAGE;

			description {
				state: "default" 0.0;
				max: 24 24;
				aspect: 1.0 1.0;
				aspect_preference: BOTH;

				image {
					normal: "wallpaper_screen_grad.png";
				}
			}
		}

		part {
			name: "screen";
			type: GRADIENT;
			mouse_events: 0;

			description {
				state: "default" 0.0;
            
				rel1 {
					to: "bg";
					offset: 5 5;
				}

				rel2 {
					to: "bg";
					offset: -6 -6;
				}

				gradient {
					spectrum: "black_to_trans";
					rel1.relative: 0 0.5;
					rel1.offset: 0 0;
					rel2.relative: 1 0.5;
					rel2.offset: -1 0;
				}
			}
		}
	}
}

