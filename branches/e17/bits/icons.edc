group {
	name: "e/icons/enlightenment/e";
	max: 256 256;

	parts {
		part {      
			name: "e";
			mouse_events: 0;

			description {
				state: "default" 0.0;
				aspect: 1.0 1.0;
				aspect_preference: BOTH;
				max: 256 256;

				image {
					normal: "e17_logo_small.png";
				}
			}
		}

		part {
			name: "e_glow";
			mouse_events: 0;

			description {
				state: "default" 0.0;
				color: 255 255 255 0;
				visible: 0;

				rel1 {
					to: "e";
				}

				rel2 {
					to: "e";
				}


				image {
					normal: "e17_icon_e_glow_0.png";
				}
			}

			description {
				state: "hilited" 0.0;
				inherit: "default" 0.0;
				color: 255 255 255 128;
				visible: 1;
			}
		}
	}

	programs {
		program {	 
			name: "e_in";
			signal: "e,state,selected";
			source: "e";
			action: STATE_SET "hilited" 0.0;
			transition: DECELERATE 0.5;
			target: "e_glow";
		}

		program {
			name: "e_out";
			signal: "e,state,unselected";
			source: "e";
			action: STATE_SET "default" 0.0;
			transition: DECELERATE 1.0;
			target: "e_glow";
		}
	}
}

