part {
	name: "cpu";
	type: IMAGE;
	mouse_events: 1;
	clip_to: "fade_clip";

	description {
		state: "default" 0.0;
		aspect: 1.0 1.0;
		aspect_preference: BOTH;
	
	r1 { relative: 0.0 0.0; offset: 0 0; }
	r2 { relative: 1.0 1.0; offset: -1 -1; }

	image {
		normal: "devices-cpu-48.png";
	}
    }
}

part {
	 name: "meter";
	 type: IMAGE;
         clip_to: "fade_clip";
	 mouse_events: 0;
	 
	 description {
	    state: "default" 0.0;

	r1 { to: "cpu"; relative: 0.7 0.1; offset: 0 0; } 
	r2 { to: "cpu"; relative: 0.9 0.9; offset: -1 -1; }

		image {
