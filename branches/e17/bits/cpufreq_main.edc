part {
	name: "readout";
	type: TEXT;
	effect: SHADOW;
	mouse_events: 0;
	clip_to: "fade_clip";

	description {
		state: "default" 0.0;

	r1 { relative: 0.5 0.7; offset: 0 0; }
	r2 { relative: 0.5 0.7; offset: 0 0; }

	color: 60 60 60 255;
	color3: 170 168 169 128;

	text {
		text: "??? Ghz";
		font: "Edje-Vera-Bold";
		size: 10;
		align: 0.5 0.5;
		min: 1 1;
		text_class: "module_small";	       
	}
    }
}
      
