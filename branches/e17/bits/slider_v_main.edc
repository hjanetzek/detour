part {
	name: "slider_image";
	mouse_events: 0;

	description {
		state: "default" 0.0;
		min: 17 29;
		max: 17 29;
		fixed: 1 1;

	r1 { to: "e.dragable.slider"; }
	r2 { to: "e.dragable.slider"; }

	image {
		normal: "slider_vert0.png";
	}
}

	description {
		state: "clicked" 0.0;
		inherit: "default" 0.0;
	
	image {
		normal: "slider_vert1.png";
	}
    }

	description {
		state: "disabled" 0.0;
		inherit: "default" 0.0;
	
	image {
		normal: "slider_vert2.png";
	}
    }
}

part {
	name: "clip";
	type: RECT;

	description {
		state: "default" 0.0;
	}

	description {
		state: "disabled" 0.0;
		inherit: "default" 0.0;
		color: 255 255 255 128;
	}
}

part {
	name: "catch";
	type: RECT;
	mouse_events: 1;

	description {
		state: "default" 0.0;
		 visible: 0;
		color: 0 0 0 0;
	}
	
	description {
		state: "disabled" 0.0;
		inherit: "default" 0.0;
		visible: 1;
	}
}

part {
	name: "focus";
	type: IMAGE;
	mouse_events: 0;

	description {
		state: "default" 0.0;
		visible: 0;
		max: 20 9999;

	r1 { to: "slider_base"; offset: -2 3; }
	r2 { to: "slider_base"; offset: 0 -4; }

	image {
		normal: "slider_focus.png";
		border: 5 5 5 5;
	}
} 

	description {
		state: "active" 0.0;
		inherit: "default" 0.0;
		visible: 1;
	}
}
