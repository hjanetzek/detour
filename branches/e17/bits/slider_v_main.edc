
part {
	name: "focus";
	type: IMAGE;
	mouse_events: 0;

	description {
		state: "default" 0.0;
		max: 20 9999;
		visible: 0;

		rel1 { 
			to: "slider_base"; 
			offset: -2 3; 
		}

		rel2 { 
			to: "slider_base"; 
			offset: 0 -4; 
		}

		image {
			normal: "slider_focus.png";
			border: 5 5 5 5;
		}
	} 

	description {
		state: "active" 0.0;
		inherit: "default" 0.0;
		visible: 1;
	}
}

part {
	name: "slider_base";
	type: RECT;
	mouse_events: 1;

	description {
		state: "default" 0.0;
		color: 255 0 0 0;

		rel1 { 
			offset: 0 0; 
		}

		rel2 { 
			offset: -1 -1; 
		}
	}

	description {
		state: "active" 0.0;
		color: 255 0 0 0;

		rel1 { 
			offset: 2 2; 
		}

		rel2 { 
			to_y: "label_under"; 
			relative: 1.0 0.0; 
			offset: -3 -1; 
		}
	}
}

part {
	name: "slider_runner";
	mouse_events: 0;
	clip_to: "clip";

	description {
		state: "default" 0.0;
		max: 11 99999;

		rel1 { 
			to: "runner_top"; 
			relative: 0.0 1.0; 
			offset: -1 0; 
		}

		rel2 { 
			to: "runner_bot"; 
			relative: 1.0 1.0; 
			offset: 0 -7; 
		}

		image {
			normal: "sb_runnerv.png";
			border: 2 2 1 1;
		}

		fill {
			smooth: 0;
		}
	}

	description {
		state: "disabled" 0.0;
