			normal: "sb_runnerv_disabled.png";
		}
	}
}

part {
	name: "label_under";
	mouse_events: 0;

	description {
		state: "default" 0.0;
		min: 27 22;
		max: 9999 22;
		visible: 0;

		image {
			normal: "entry.png";
			border: 5 5 8 7;
		}
	}

	description {
		state: "active" 0.0;
		min: 27 22;
		max: 9999 22;
		visible: 1;

		rel1 { 
			to: "e.text.label"; 
			relative: 0.0 0.0; 
			offset: -7 -4; 
		}

		rel2 { 
			to: "e.text.label"; 
			relative: 1.0 1.0; 
			offset: 3 3; 
		}

		image {
			normal: "entry.png";
			border: 5 5 8 7;
		}

		fill {
			smooth: 0;
		}
	}
}

part {
	name: "e.text.label";
	type: TEXT;
	mouse_events: 0;
	clip_to: "clip";

	description {
		state: "default" 0.0;
		visible: 0;
	}

	description {
		state: "active" 0.0;
		align: 0.5 1.0;
		color: 60 60 60 255;

		rel1 { 
			relative: 0.0 1.0; 
			offset: 7 -8; 
		}

		rel2 { 
			relative: 1.0 1.0; 
			offset: -8 -8; 
		}

		text {
			text: "00.00";
			font: "Edje-Vera";
			size: 10;
			min: 1 1;
			align: 0.5 0.5;
			#if ENABLE_TEXT_CLASSES == 0
				text_class: "";
			#else
			#if ENABLE_TEXT_CLASSES == 1
				text_class: "slider";
			#endif
			#endif
		}
	}
}

