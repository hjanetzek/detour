
program { 
	name: "sb_hbar_show";
	signal: "e,action,show,hbar";
	source: "e";
	action:  STATE_SET "default" 0.0;
	transition: LINEAR 0.5;
	target: "left_arrow";
	target: "right_arrow";
}

program { 
	name: "sb_hbar_hide";
	signal: "e,action,hide,hbar";
	source: "e";
	action:  STATE_SET "hidden" 0.0;
	target: "left_arrow";
	target: "right_arrow";
	transition: LINEAR 0.5;
}

