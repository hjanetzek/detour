
program {
	name: "entry_focus";
	signal: "e,state,focused";
	source: "e";
	action: STATE_SET "focused" 0.0;
	transition: DECELERATE 0.25;
	target: "entry";
}

program {
	name: "entry_unfocus";
	signal: "e,state,unfocused";
	source: "e";
	action: STATE_SET "default" 0.0;
	transition: DECELERATE 0.25;
	target: "entry";
}
