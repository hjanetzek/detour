
group {
	name: "e/modules/battery/main";
	max: 85 85;
	min: 1 1;

		script {
			public message(Msg_Type:type, id, ...) {
				if ((type == MSG_FLOAT) && (id == 1)) {
				new Float:val;
				val = getfarg(2);
				set_state(PART:"battery", "default", val);
				}
			}
		}

	parts {
		#include "battery_main.edc"
	}

	programs {
		#include "battery_programs.edc"
	}
}

group {
	name: "e/modules/battery/popup";
	min:  240 64;
	max:  420 96;

	parts {
		#include "battery_popup.edc"
	}
}

