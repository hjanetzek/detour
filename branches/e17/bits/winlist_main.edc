		state: "default" 0.0;

	r1 { to: "e.swallow.icon"; relative: 0.0 0.0; offset: -8 -8; }
	r2 { to: "e.swallow.icon"; relative: 1.0 1.0; offset: 7 7; }

	image {
		normal: "entry.png";
		border: 5 5 8 6;
	}
    }
}

