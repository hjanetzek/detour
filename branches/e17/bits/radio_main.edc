part {
	name: "e.text.label";
	type: TEXT;
	mouse_events: 0;

	description {
		state: "default" 0.0;
		min: 17 17;
		color: 60 60 60 255;

		rel1 {
			to_x: "radio";
			relative: 1.0 0.0;
			offset: 5 2;
		}

		rel2 {
			relative: 1.0 1.0;
			offset: -5 -2;
		}

		text {
			text: "Check Button";
			font: "DejaVuSans";
			size: 11;
			min: 1 1;
			align: 0.0 0.5;
