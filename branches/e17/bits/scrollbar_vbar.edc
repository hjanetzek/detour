part {
	name: "e.dragable.vbar.clip";
	clip_to: "sb_vbar";
	mouse_events: 0;

	description {
		state: "default" 0.0;
		min: 13 36;
		max: 13 9999;
		color: 255 255 255 255;

		rel1 { 
			to: "sb_vbar_base"; 
			relative: 0.0 0.0; 
			offset: 0 0; 
		}

		rel2 { 
			to: "sb_vbar_base"; 
			relative: 1.0 1.0; 
			offset: 0 0; 
		}
	}

	description {
		state: "clicked" 0.0;
		inherit: "default" 0.0;
		color: 215 215 215 255;
	}
}

part {
	name: "e.dragable.vbar";
	clip_to: "sb_vbar";
	clip_to: "e.dragable.vbar.clip";
	mouse_events: 1;

	dragable {
		x: 0 0 0;
		y: 1 1 0;
		confine: "sb_vbar_base";
	}

	description {
		state: "default" 0.0;
		min: 13 36;
		max: 13 9999;
		color: 255 255 255 255;

		rel1 { 
			to: "sb_vbar_base"; 
			relative: 0.5 0.5; 
			offset: 0 0; 
		}

		rel2 { 
			to: "sb_vbar_base"; 
			relative: 0.5 0.5; 
			offset: 0 0; 
		}

		image {
			normal: "sf_barv1.png";
			border: 6 6 6 6;
		}
	}

	description {
		state: "clicked" 0.0;
		inherit: "default" 0.0;
		color: 215 215 215 255;
	}
}

