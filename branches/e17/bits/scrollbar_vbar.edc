
part {
	name: "sb_vbar";
	type: RECT;
	mouse_events: 0;

	description {
		state: "default" 0.0;
		min: 19 19;
		align: 1.0 0.0;
		color: 255 255 255 255;

		rel1 { 
			relative: 1.0 0.0; 
			offset: 0 -1; 
		}

		rel2 { 
			relative: 1.0 0.0; 
			offset: 1 1; 
			to_y: "sb_hbar"; 
		}
	}

	description {
		state: "hidden" 0.0;
		max: 0 99999;
		visible: 0;

		rel1 { 
			relative: 1.0 0.0; 
			offset: 0 1; 
		}

		rel2 { 
			relative: 1.0 0.0; 
			offset: -3 0; 
			to_y: "sb_hbar"; 
		}
	}
}

part {
	name: "sb_vbar_base";
	type: RECT;
	clip_to: "sb_vbar";
	mouse_events: 0;

	description {
		state: "default" 0.0;
		color: 255 0 0 0;

		rel1 { 
			to: "sb_vbar_a1"; 
			relative: 0.0 1.0; 
			offset: 0 0; 
		}

		rel2 { 
			to: "sb_vbar_a2"; 
			relative: 1.0 0.0; 
			offset: -1 -1; 
		}
	}
}

