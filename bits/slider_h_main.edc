part {
	name: "confine_area";
	type: RECT;
	mouse_events: 0;

	description {
		state: "default" 0.0;
		max: 99999 11;

	r1 { to: "runner_left"; relative: 1.0 0.0; offset: -2 18; }
	r2 { to: "runner_right"; relative: 0.0 1.0; offset: 2 0; }

	color: 255 0 0 0;
	}
}

part {
	name: "e.dragable.slider";
	type: RECT;
	mouse_events: 1;

	dragable {
		x: 1 1 0;
		y: 0 0 0;
		confine: "confine_area";
	}

	description {
		state: "default" 0.0;
		min: 29 17;
		max: 29 17;
		fixed: 1 1;

	r1 { to: "slider_base"; relative: 0.5 0.5; offset: 0 0; }
	r2 { to: "slider_base"; relative: 0.5 0.5; offset: 0 0; }

	color: 255 0 0 0;
	}
}

